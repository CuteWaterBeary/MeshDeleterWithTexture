#pragma kernel CSMain

Texture2D<float4> Tex;

int Width;
int Height;

RWTexture2D<float4> Result;
AppendStructuredBuffer<float2> ContourPos;

[numthreads(1,1,1)]
void CSMain (uint2 id : SV_DispatchThreadID)
{
	float gray = Tex[id].r + Tex[id].g + Tex[id].b / 3.f;
	Result[id] = float4(gray, gray, gray, 1);
	
	GroupMemoryBarrierWithGroupSync();


}
